{
    "name": "elkebirmed/adminior",
    "description": "Laravel admin panel",
    "license": "MIT",
    "authors": [
        {
            "name": "Mohamed Elkebir",
            "email": "elkebir.med@gmail.com",
            "homepage": "https://github.com/elkebirmed"
        }
    ],
    "homepage": "https://github.com/elkebirmed/adminior",
    "keywords": ["Laravel", "Adminior"],
    "require": {
        "illuminate/support": "~7|~8"
    },
    "require-dev": {
        "phpunit/phpunit": "~9.0",
        "orchestra/testbench": "~5|~6"
    },
    "autoload": {
        "psr-4": {
            "Elkebirmed\\Adminior\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Elkebirmed\\Adminior\\Tests\\": "tests"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "Elkebirmed\\Adminior\\AdminiorServiceProvider"
            ],
            "aliases": {
                "Adminior": "Elkebirmed\\Adminior\\Facades\\Adminior"
            }
        }
    }
}
